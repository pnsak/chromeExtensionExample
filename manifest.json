{
    "name": "Track Progess",
    "version": "1.0",
    "description": "Track your learning progress ubiquitously",
    "manifest_version": 2,
    "content_scripts": 
        [
            {
                "matches":["<all_urls>"],
                "js": ["content.js"]
            }
        ],
    "background": 
        {
            "scripts": ["background.js"]
        },
    "browser_action":
        {
            "default_icon": "ct.png"
        }
}
